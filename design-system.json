      }
    },
    "12": {
      "level": 12,
      "shadow": "$shadows.xl",
      "ios": {
        "shadowColor": "#000000",
        "shadowOffset": { "width": 0, "height": 6 },
        "shadowOpacity": 0.37,
        "shadowRadius": 7.49
      },
      "android": {
        "elevation": 12
      },
      "web": {
        "boxShadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1)"
      }
    },
    "16": {
      "level": 16,
      "shadow": "$shadows.2xl",
      "ios": {
        "shadowColor": "#000000",
        "shadowOffset": { "width": 0, "height": 8 },
        "shadowOpacity": 0.44,
        "shadowRadius": 10.32
      },
      "android": {
        "elevation": 16
      },
      "web": {
        "boxShadow": "0 25px 50px -12px rgba(0, 0, 0, 0.25)"
      }
    },
    "24": {
      "level": 24,
      "shadow": "$shadows.2xl",
      "ios": {
        "shadowColor": "#000000",
