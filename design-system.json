      },
      "android": {
        "elevation": 5
      },
      "web": {
        "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -2px rgba(0, 0, 0, 0.05)"
      }
    },
    "6": {
      "level": 6,
      "shadow": "$shadows.lg",
      "ios": {
        "shadowColor": "#000000",
        "shadowOffset": { "width": 0, "height": 3 },
        "shadowOpacity": 0.27,
        "shadowRadius": 4.65
      },
      "android": {
        "elevation": 6
      },
      "web": {
        "boxShadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1)"
      }
    },
    "8": {
      "level": 8,
      "shadow": "$shadows.xl",
      "ios": {
        "shadowColor": "#000000",
        "shadowOffset": { "width": 0, "height": 4 },
